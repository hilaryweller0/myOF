FoamFile
{
    version         1.4;
    format          ascii;

    root            "/export/lime/data-04/sws02hs/OpenFOAM/sws02hs-1.4.1/run/shallowWater";
    case            "bucky4_30";
    instance        "constant";
    local           "";

    class           dictionary;
    object          gmtDict;
}

// How to plot
projection "Q180/18c";
//projection "G122/30/18c";
projection2D latLon;

FieldsToPlot
2
(
    [vorticity filledContours -5.25e-5 5.25e-5 5e-6 "$GMTU/colours/red_white_blue.cpt"]
    [h0 contours 100 1600 800 "black"]
)

// options
region "-180/180/-90/90";
boundaryMarks "g0/g98";
annotate "TIME 0 -92";
