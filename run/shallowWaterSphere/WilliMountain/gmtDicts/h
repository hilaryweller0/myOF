FoamFile
{
    version         1.4;
    format          ascii;

    root            "/export/lime/data-04/sws02hs/OpenFOAM/sws02hs-1.4.1/run/shallowWater";
    case            "bucky4_30";
    instance        "constant";
    local           "";

    class           dictionary;
    object          gmtDict;
}

// How to plot
projection "Q180/18c";
//projection "G45/30/18c";
projection2D latLon;

FieldsToPlot
1
(
    [h rawValues 5000 6000 100 "$GMTU/colours/red_white_blue.cpt"]
)

// options
region "-180/180/-90/90";
boundaryMarks "g0/g98";
annotate "minMax 0 -92";
//separateLegend true;
//overlay true;
