FoamFile
{
    version         1.4;
    format          ascii;

    root            "/export/lime/data-04/sws02hs/OpenFOAM/sws02hs-1.4.1/run/shallowWater";
    case            "bucky4_30";
    instance        "constant";
    local           "";

    class           dictionary;
    object          gmtDict;
}

// How to plot
projection "Q180/9c";
//projection "G90/30/18c";
projection2D latLon;

FieldsToPlot
3
(
    [hError  rawValues -24 24 2 "$GMTU/colours/red_white_blue.cpt"]
    [hTotal  contours 5000 6000 50 "black"]
    [h0 contours 100 2000 2000 "1,black"]
);

// options
region "-180/180/-90/90";
boundaryMarks "g0/g89";
separateLegend true;
annotate "minMax 0 -92";

