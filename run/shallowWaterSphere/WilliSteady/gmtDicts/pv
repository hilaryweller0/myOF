FoamFile
{
    version         1.4;
    format          ascii;

    root            "/export/lime/data-04/sws02hs/OpenFOAM/sws02hs-1.4.1/run/shallowWater";
    case            "bucky4_30";
    instance        "constant";
    local           "";

    class           dictionary;
    object          gmtDict;
}

// How to plot
//projection "Q0/18c";
projection "G90/30/18c";
projection2D latLon;

FieldsToPlot
2
(
    [pv rawValues -3.0e-7 3.0e-7 5.e-8 "$HOME/gmt/usr/colours/red_white_blue"]
    [Uf    vectors   0.2 1 "black"]
);

// options
region "-180/180/-90/90";
boundaryMarks "g0/g89";
