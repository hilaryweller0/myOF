FoamFile
{
    version         1.4;
    format          ascii;

    root            "/export/lime/data-04/sws02hs/OpenFOAM/sws02hs-1.4.1/run/shallowWater";
    case            "bucky4_30";
    instance        "constant";
    local           "";

    class           dictionary;
    object          gmtDict;
}

// How to plot
projection "Q0/18c";
//projection "G90/30/18c";
