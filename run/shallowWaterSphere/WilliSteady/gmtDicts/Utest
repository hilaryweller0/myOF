//patch originalPatch;

FieldsToPlot
4
(
    [none mesh "grey"]
    [Uf vectors   50 1 "black"]
    [Urecon vectors 50 1 "blue"]
    [Uerror vectors 50 1 "red"]
);

// options
region "-180/180/-90/90";
boundaryMarks "g0/g90";
annotate "minMax 0 -0.01";
