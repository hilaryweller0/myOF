FoamFile
{
    version         1.4;
    format          ascii;

    root            "/export/lime/data-04/sws02hs/OpenFOAM/sws02hs-1.4.1/run/shallowWater";
    case            "bucky4_30";
    instance        "constant";
    local           "";

    class           dictionary;
    object          gmtDict;
}

// How to plot
projection "Q0/18c";
//projection "G90/30/18c";
projection2D latLon;
//patch originalPatch;

FieldsToPlot
2
(
    [KE rawValues 0 0 0 "$GMTU/colours/red_white_blue.cpt"]
    [KE contours 0 1600 100 "black"]
);

// options
region "-180/180/-90/90";
boundaryMarks "g0/g90";
annotate "minMax 0 -0.01";

