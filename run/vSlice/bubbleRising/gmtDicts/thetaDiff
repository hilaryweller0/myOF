FoamFile
{
    version         2.3;
    format          ascii;

    root            "";
    case            "";
    instance        "constant";
    local           "";

    class           dictionary;
    object          gmtDict;
}

// How to plot
// Compulsory inputs
projection "X9c/9c";
projection2D XZ;

FieldsToPlot
1
(
    [thetaDiff filledContours -1.05 1.05 0.1 "$GMTU/colours/red_white_blue.cpt"]
);

separateLegend true;
region "-5000/5000/2500/12500";
annotate "minMax -5e3 1.7e3";
boundaryMarks "2500/2500";

