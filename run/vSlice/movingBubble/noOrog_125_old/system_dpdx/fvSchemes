FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    class           dictionary;
    object          fvSchemes;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default Euler;
}
offCentre 0.5;
offCentreTheta 0.5;

SIgravityWaves true;

gradSchemes
{
    default         none;
    grad(theta)     Gauss cubicUpwindFit phi 3;
}

divSchemes
{
    default             none;
    div(phi,theta)      Gauss cubicUpwindFit 3;
    div(phi,U)          Gauss cubicUpwindFit 3;
}

laplacianSchemes
{
    default         Gauss linear linearFit 3;
}

interpolationSchemes
{
    default        none; 
    interpolate(theta)    midPoint;
    interpolate(rho)    midPoint;
    interpolate(convection(phi,U)) midPoint;
    interpolate(U) midPoint;
    interpolate(grad(theta)) midPoint;
}

snGradSchemes
{
    default         none;
    snGrad(Exner)   linearFit 3;
    snGrad(theta)   linearFit 3;
}

fluxRequired
{
    default         no;
    Exner;
}


