FoamFile
{
    version         2.3;
    format          ascii;

    root            "";
    case            "";
    instance        "constant";
    local           "";

    class           dictionary;
    object          gmtDict;
}

// How to plot
// Compulsory inputs
projection "X18c/9c";
projection2D XZ;

FieldsToPlot
4
(
    [theta filledContours 298.99 301.01 0.02 "$GMTU/colours/red_white_blue.cpt"]
    [UfDiff vectors 50 20 "black"]
    [Ufz contours 0 30 2 "2pt,black"]
    [Ufz contours -30 -2 2 "2pt,black,15_15:0"]
);

separateLegend true;
region "-15e3/15e3/0/15e3";
annotate "minMax -15e3 -1/5e3";
boundaryMarks "5e3/5e3";

