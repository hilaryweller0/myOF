FoamFile
{
    version         1.4;
    format          ascii;

    root            "";
    case            "";
    instance        "constant";
    local           "";

    class           dictionary;
    object          gmtDict;
}

// How to plot
// Compulsory inputs
projection "X18c/6c";
projection2D XZ;
patch front;

FieldsToPlot
4
(
    [thetaDiff filledContours -4e-3 4e-3 5e-4 "rainbow"]
    [thetaDiff contours .5e-3 1e-2 .5e-3 "2pt,black"]
    [thetaDiff contours 0 1 1 "2pt,black,2_6:0"]
    [thetaDiff contours -1 -.5e-3 .5e-3 "2pt,black,30_30:0"]
);

boundaryMarks "a5e5/a2e3";
separateLegend true;
annotate "minMax 0.01 -0.005";

