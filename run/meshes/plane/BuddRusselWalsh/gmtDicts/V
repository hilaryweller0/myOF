FoamFile
{
    version         2.2;
    format          ascii;

    root            "";
    case            "";
    instance        "constant";
    local           "";

    class           dictionary;
    object          gmtDict;
}

// How to plot
// Compulsory inputs
projection "X18c/18c";
projection2D XZ;

FieldsToPlot
2
(
    [volRatio rawValues 0 2.6 .1 "$GMTU/colours/white_blue_red.cpt"]
    [none mesh "grey"]
);

annotate "minMax 0 -0.001";
region "0/0.5/0/0.5";
separateLegend true;
