FoamFile
{
    version         2.2;
    format          ascii;

    root            "";
    case            "";
    instance        "constant";
    local           "";

    class           dictionary;
    object          gmtDict;
}

// How to plot
// Compulsory inputs
projection "X18c/18c";
projection2D XZ;

FieldsToPlot
1
(
    [monitorR rawValues 0 0 0 "$GMTU/colours/red_white_blue.cpt"]
);

annotate "minMax 0 -0.001";
region "0/0.5/0/0.5";

