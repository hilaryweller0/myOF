FoamFile
{
    version         1.4;
    format          ascii;

    root            "/export/lime/data-04/sws02hs/OpenFOAM/sws02hs-1.4.1/run/shallowWater";
    case            "bucky4_30";
    instance        "constant";
    local           "";

    class           dictionary;
    object          gmtDict;
}

// How to plot
projection "Q0/18c";
//projection "G90/30/18c";
projection2D latLon;

FieldsToPlot
2  
(
    [hDiff  rawValues -200 200 40 "$GMTU/colours/red_white_blue.cpt"]
//    [none mesh      "3pt,darkgrey"]
    [h0    contours   1 2000 2000 "black"]
);

// options
region "-180/180/-90/90";
boundaryMarks "g0/g89";
//separateLegend true;
annotate "minMax 0 0";
